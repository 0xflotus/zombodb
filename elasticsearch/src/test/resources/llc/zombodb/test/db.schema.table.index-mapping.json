  "settings": {
    "refresh_interval": -1,
    "number_of_shards": 5,
    "number_of_replicas": 0,
    "analysis": {
      "normalizer": {
        "exact": {
          "type": "custom",
          "char_filter": [],
          "filter": ["lowercase"]
        }
      },
      "filter": {
        "truncate_32000": {
          "type": "truncate",
          "length": 32000
        },
        "shingle_filter": {
          "type": "shingle",
          "min_shingle_size": 2,
          "max_shingle_size": 2,
          "output_unigrams": true,
          "token_separator": "$"
        },
        "shingle_filter_search": {
          "type": "shingle",
          "min_shingle_size": 2,
          "max_shingle_size": 2,
          "output_unigrams": false,
          "token_separator": "$"
        },
        "issue349_ngram_filter": {
          "type": "edgeNGram",
          "min_gram": 3,
          "max_gram": 8
        }
      },
      "analyzer": {
        "default": {
          "tokenizer": "keyword",
          "filter": [
            "truncate_32000",
            "lowercase"
          ]
        },
        "exact": {
          "tokenizer": "keyword",
          "filter": [
            "truncate_32000",
            "lowercase"
          ]
        },
        "phrase": {
          "tokenizer": "standard",
          "filter": [
            "lowercase"
          ]
        },
        "fulltext": {
          "tokenizer": "standard",
          "filter": [
            "lowercase"
          ]
        },
        "fulltext_with_shingles": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "shingle_filter"
          ]
        },
        "fulltext_with_shingles_search": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "shingle_filter_search"
          ]
        },
        "issue349_analyzer": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": ["lowercase", "asciifolding","issue349_ngram_filter"]
        }
      }
    }
